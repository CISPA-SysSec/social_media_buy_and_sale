{
    "0": "\u0645\u0628\u0627\u0631\u0629 \u0627\u0644\u0628\u0631\u062a\u063a\u0627\u0644",
    "1": "\u0644\u0627\u062c\u0648\u062f\u0627\u0646 \u0627\u062f\u0631\u064a\u0633 \u062d\u062c\u064a : \u0634\u0639\u0644\u0648 \u0627\u0644\u0639\u0627\u0641.\u064a\u0629 \u0641\u062e\u0645\u0633\u0629 \u0628\u062f\u0627\u0641\u0639 \u0627\u0644\u0627\u0646\u062a\u0642\u0627\u0645",
    "2": "\u0647\u0627 \u0643\u064a\u0641\u0627\u0634 \u0643\u064a\u0647\u0631\u0628\u0648 \u0627\u0644\u062d--\u0634\u064a-\u0634 \u0627\u0644\u0649 \u0627\u0644\u062e\u0627\u0631\u062c \u0645\u0639 \u0644\u0627\u062c\u0648\u062f\u0627\u0646 \u0627\u062f\u0631\u064a\u0633 \u062d\u062c\u064a",
    "3": "\u0648\u0644\u064a\u062f \u0627\u0644\u0631\u0643\u0631\u0627\u0643\u064a :\u0623\u0646\u0627 \u0641\u0631\u062d\u0627\u0646 \u0628\u0632\u0627\u0641 \u0644\u0644\u0645\u0628\u0627\u0631\u0627\u0629 \u0644\u064a \u062f\u0627\u0631\u0646\u0627 \u0648\u0627\u0644\u0645\u0646\u062a\u062e\u0628 \u0644\u064a \u063a\u062a\u0634\u0648\u0641\u0648\u0647  \u0641 \u0633\u064a\u0628\u062a\u0645\u0628\u0631 \u0645\u0627\u0634\u064a \u0647\u0648 \u0647\u0630\u0627 \ud83c\uddf2\ud83c\udde6\ud83d\udc4f",
    "4": "\u0633\u064a\u062a\u0645 \u0646\u0642\u0644 \u0627\u0644\u0645\u0628\u0627\u0631\u0629 \u0639\u0628\u0631 \u0635\u0641\u062d\u062a\u0646\u0627 \u0627\u0644\u0631\u0633\u0645\u064a\u0629\n\u062f\u064a\u0644\u064a \u0633\u0628\u0648\u0631\u062a/Daily sports",
    "5": "\u0645\u0628\u0627\u0631\u0629 \u0628\u0631\u0634\u0644\u0648\u0646\u0647 \u0648 \u0645\u0627\u0646\u0634\u0633\u062a\u0631 \u064a\u0648\u0646\u0627\u064a\u062a\u062f",
    "6": "\u0645\u062d\u0628\u064a \u0627\u0644\u0642\u0635\u0635 \u0627\u0644\u0645\u0634\u0648\u0642\u0629 \u0648 \u0627\u0644\u0628\u0648\u0644\u064a\u0633\u064a\u0629",
    "7": "\u062a\u0623\u0646\u064a\u0628 \u0627\u0644\u0636\u0645\u064a\u0631",
    "8": "\u0627\u0644\u0623\u0646 \u0645\u0628\u0627\u0631\u0629 \u0644\u0628\u064a\u0627 \u0639\u0628\u0631 \u0635\u0641\u062d\u062a\u0646\u0627 \u0627\u0644\u0631\u0633\u0645\u064a\u0629 \u0641\u0631\u062c\u0629 \u0645\u0645\u062a\u0639\u0629",
    "9": "\u270d\ufe0f \u0633\u064a\u062a\u0645 \u0646\u0642\u0644 \u0645\u0628\u0627\u0631\u0629 \u0627\u0644\u0645\u063a\u0631\u0628 \u0648 \u062c\u0646\u0648\u0628 \u0627\u0641\u0631\u064a\u0642\u064a\u0627  \u0639\u0628\u0631 \u0635\u0641\u062d\u062a\u0646\u0627 \u0627\u0644\u0631\u0633\u0645\u064a\u0629 \ud83d\ude0d",
    "10": "\u0627\u0633\u0631\u0639 5 \u0644\u0627\u0639\u0628\u064a\u0646 \u0641\u064a \u0627\u0644\u0639\u0627\u0644\u0645",
    "11": "\u0635\u0641\u0627\u0647\u0627 \u0644\u0635\u0627\u062d\u0628\u0648 \u062d\u064a\u062a \u062e\u0627\u0646\u0648",
    "12": "\u0647\u0627 \u0643\u064a\u0641\u0627\u0634 \u0633\u0631\u0642\u0648\u0627 \u062c\u0648\u062c \u0648\u0643\u0627\u0644\u0627\u062a \u0628\u0631\u064a\u062f\u064a\u0629 \u0628\u0637\u0631\u064a\u0642\u0629 \u0627\u062d\u062a\u0631\u0627\u0641\u064a\u0629"
}